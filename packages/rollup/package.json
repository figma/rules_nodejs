{
  "name": "@bazel/rollup",
  "dependencies": {
    "@bazel/worker": "0.0.0-PLACEHOLDER"
  },
  "peerDependencies": {
    "rollup": ">=2.3.0 <3.0.0"
  },
  "description": "Run rollup.js bundler under Bazel",
  "license": "Apache-2.0",
  "version": "0.0.0-PLACEHOLDER",
  "repository": {
    "type": "git",
    "url": "https://github.com/bazelbuild/rules_nodejs.git",
    "directory": "packages/rollup"
  },
  "bin": {
    "rollup-worker": "index.js"
  },
  "bugs": {
